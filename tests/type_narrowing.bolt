@MyStruct := struct {
    a : i32
}

do_i32(val : i32) : i32 {
    return val
}

do_i64(val : i64) : i64 {
    return val
}

abc(@T : type, val : T) : i32 {

    if (T == i32) {
        return do_i32(val)
    }

    if (T == i64) {
        do_i64(val)
        return -1
    }

    return 0
}

main() : i32 {

    abc(i32, 1)
    abc(i64, 2)

    return 0
}
