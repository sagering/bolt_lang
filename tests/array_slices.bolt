
extern write(fd : i32, buffer : *u8, count : u64) : u64

print(str : []u8, len : i32) : i32 {
    write(1, &str[0], 5)
    return 0
}

main() : i32 {
    arr : [7]u8 = [72, 101, 108, 108, 111, 33, 0]
    arr_part : []u8 = arr[:5]
    i : u8 = 0
    while i < 5 {
        print(arr_part, 5)
        i = i + 1
    }
    return 1
}
