@ints : typeset = i8 | i32 | i64 | u8 | u32 | u64

sum(@T : ints, vals : []T) : T {
    result := T{}
    i := 0
    while i < len(vals) {
        result = result + vals[i]
        i = i + 1
    }
    return result
}

test2() : i32 {
    vals : [2]i32 = [0, 1]
    return sum(i32, vals[:])
}

main() : i32 {
    test2()
    return 0
}